<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#C4CDE0"/>
            <corners android:radius="60dp"/>
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:top="1dp"
        android:right="1dp"
        android:left="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#EEF1F9" />
            <corners android:radius="60dp"/>
        </shape>
    </item>
</layer-list>